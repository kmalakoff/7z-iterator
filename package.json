{
  "name": "7z-iterator",
  "version": "0.1.3",
  "description": "Extract contents from 7z archives using an iterator API. Pure JavaScript, works on Node.js 0.8+",
  "keywords": [
    "extract",
    "iterator",
    "asyncIterator",
    "Symbol.asyncIterator",
    "7z",
    "7zip",
    "seven-zip",
    "decompress",
    "lzma",
    "lzma2",
    "archive",
    "stream"
  ],
  "homepage": "https://github.com/kmalakoff/7z-iterator",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/7z-iterator.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe",
    "prepublishOnly": "tsds validate",
    "test": "mocha --no-timeouts test/**/*.test.*",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "call-once-fn": "*",
    "extract-base-iterator": "*",
    "lodash.compact": "*",
    "lzma-purejs": "^0.9.3",
    "mkdirp-classic": "*",
    "on-one": "*",
    "os-shim": "*",
    "pako": "^1.0.11",
    "queue-cb": "*",
    "readable-stream": "^2.3.7",
    "rimraf2": "*",
    "seek-bzip": "^2.0.0",
    "short-hash": "*",
    "temp-suffix": "*"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "array-find": "^1.0.0",
    "fs-iterator": "*",
    "fs-stats-spys": "*",
    "get-remote": "^2.2.1",
    "lzma-native": "^8.0.6",
    "pinkie-promise": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
